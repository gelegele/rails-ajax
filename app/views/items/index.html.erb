<h1>Listing items</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Sex</th>
    <th>Birth</th>
    <th>Note</th>
    <th>Start time</th>
    <th>Registered</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.name %></td>
    <td><%= item.sex %></td>
    <td><%= item.birth %></td>
    <td><%= item.note %></td>
    <td><%= item.start_time %></td>
    <td><%= item.registered %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
